{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "additionalProperties": false,
    "definitions": {
        "AgentBotFassetSettingsJson": {
            "additionalProperties": false,
            "properties": {
                "minimumFreeUnderlyingBalance": {
                    "description": "The amount of underlying currency on woner's underlying address, below which the address should be topped-up,\nto prevent negative free underlying balance after redemptions.",
                    "pattern": "^[0-9]+(\\.[0-9]+)?$",
                    "type": "string"
                },
                "recommendedOwnerBalance": {
                    "description": "The amount of underlying currency on woner's underlying address, below which an alert is triggered.",
                    "pattern": "^[0-9]+(\\.[0-9]+)?$",
                    "type": "string"
                }
            },
            "required": [
                "minimumFreeUnderlyingBalance",
                "recommendedOwnerBalance"
            ],
            "type": "object"
        },
        "AgentBotSettingsJson": {
            "additionalProperties": false,
            "properties": {
                "fAssets": {
                    "additionalProperties": {
                        "$ref": "#/definitions/AgentBotFassetSettingsJson"
                    },
                    "description": "Per FAsset settings.",
                    "type": "object"
                },
                "liquidationPreventionFactor": {
                    "description": "Minimum amount of collateral to topup vault to, to prevent liquidation.\nRelative to collateral's CCB CR.",
                    "pattern": "^[0-9]+(\\.[0-9]+)?$",
                    "type": "string"
                },
                "minBalanceOnServiceAccount": {
                    "description": "Minimum balance needed for gas on request submitter and timekeeper account.",
                    "type": "string"
                },
                "minBalanceOnWorkAccount": {
                    "description": "Minimum balance needed for gas and other things on agent work account.\nPool collateral topups always leav this amount.",
                    "type": "string"
                },
                "parallel": {
                    "description": "If true, mintings and various redemption steps will run in parallel.\nWARNING: should not be used with sqlite database.",
                    "type": "boolean"
                },
                "poolCollateralReserveFactor": {
                    "description": "The threshold for NAT on owner's work address, below which alert is triggered.\nRelative to required pool collateral for current minted amount.",
                    "pattern": "^[0-9]+(\\.[0-9]+)?$",
                    "type": "string"
                },
                "trustedPingSenders": {
                    "description": "The list of address to whose pings the agent will respond.",
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                },
                "vaultCollateralReserveFactor": {
                    "description": "The threshold for USDC/WETH/... on owner's work address, below which alert is triggered.\nRelative to required vault collateral for current minted amount.",
                    "pattern": "^[0-9]+(\\.[0-9]+)?$",
                    "type": "string"
                }
            },
            "required": [
                "fAssets",
                "liquidationPreventionFactor",
                "minBalanceOnServiceAccount",
                "minBalanceOnWorkAccount",
                "parallel",
                "poolCollateralReserveFactor",
                "trustedPingSenders",
                "vaultCollateralReserveFactor"
            ],
            "type": "object"
        },
        "ApiNotifierConfig": {
            "additionalProperties": false,
            "properties": {
                "apiKey": {
                    "type": "string"
                },
                "apiUrl": {
                    "type": "string"
                }
            },
            "required": [
                "apiKey",
                "apiUrl"
            ],
            "type": "object"
        },
        "BotFAssetInfo": {
            "additionalProperties": false,
            "properties": {
                "chainId": {
                    "type": "string"
                },
                "fallbackApis": {
                    "items": {
                        "$ref": "#/definitions/WalletApi"
                    },
                    "type": "array"
                },
                "faucet": {
                    "type": "string"
                },
                "feeServiceOptions": {
                    "$ref": "#/definitions/FeeServiceOptions"
                },
                "indexerUrl": {
                    "type": "string"
                },
                "minimumAccountBalance": {
                    "type": "string"
                },
                "priceChangeEmitter": {
                    "type": "string"
                },
                "stuckTransactionOptions": {
                    "$ref": "#/definitions/StuckTransaction"
                },
                "tokenDecimals": {
                    "type": "number"
                },
                "tokenName": {
                    "type": "string"
                },
                "tokenSymbol": {
                    "type": "string"
                },
                "walletUrl": {
                    "type": "string"
                }
            },
            "required": [
                "chainId",
                "priceChangeEmitter",
                "tokenDecimals",
                "tokenName",
                "tokenSymbol"
            ],
            "type": "object"
        },
        "BotNativeChainInfo": {
            "additionalProperties": false,
            "properties": {
                "chainName": {
                    "type": "string"
                },
                "faucet": {
                    "type": "string"
                },
                "finalizationBlocks": {
                    "type": "number"
                },
                "readLogsChunkSize": {
                    "type": "number"
                },
                "recommendedOwnerBalance": {
                    "type": "string"
                },
                "tokenSymbol": {
                    "type": "string"
                }
            },
            "required": [
                "chainName",
                "finalizationBlocks",
                "readLogsChunkSize",
                "tokenSymbol"
            ],
            "type": "object"
        },
        "BotStrategyDefinition": {
            "additionalProperties": false,
            "properties": {
                "className": {
                    "type": "string"
                },
                "config": {}
            },
            "required": [
                "className"
            ],
            "type": "object"
        },
        "DatabaseType": {
            "enum": [
                "mysql",
                "postgresql",
                "sqlite"
            ],
            "type": "string"
        },
        "FeeServiceOptions": {
            "additionalProperties": false,
            "properties": {
                "numberOfBlocksInHistory": {
                    "type": "number"
                },
                "rateLimitOptions": {
                    "$ref": "#/definitions/RateLimitOptions"
                },
                "sleepTimeMs": {
                    "type": "number"
                }
            },
            "required": [
                "numberOfBlocksInHistory",
                "sleepTimeMs"
            ],
            "type": "object"
        },
        "OrmConfigOptions": {
            "additionalProperties": {},
            "properties": {
                "charset": {
                    "type": "string"
                },
                "clientUrl": {
                    "type": "string"
                },
                "dbName": {
                    "type": "string"
                },
                "debug": {
                    "type": "boolean"
                },
                "host": {
                    "type": "string"
                },
                "pool": {
                    "additionalProperties": false,
                    "properties": {
                        "acquireTimeoutMillis": {
                            "type": "number"
                        },
                        "max": {
                            "type": "number"
                        },
                        "min": {
                            "type": "number"
                        }
                    },
                    "type": "object"
                },
                "port": {
                    "type": "number"
                },
                "schemaUpdate": {
                    "enum": [
                        "full",
                        "none",
                        "recreate",
                        "safe"
                    ],
                    "type": "string"
                },
                "type": {
                    "$ref": "#/definitions/DatabaseType"
                }
            },
            "required": [
                "type"
            ],
            "type": "object"
        },
        "RateLimitOptions": {
            "additionalProperties": false,
            "properties": {
                "maxRPS": {
                    "type": "number"
                },
                "maxRequests": {
                    "type": "number"
                },
                "perMilliseconds": {
                    "type": "number"
                },
                "retries": {
                    "type": "number"
                },
                "timeoutMs": {
                    "type": "number"
                }
            },
            "type": "object"
        },
        "StuckTransaction": {
            "additionalProperties": false,
            "properties": {
                "blockOffset": {
                    "type": "number"
                },
                "enoughConfirmations": {
                    "type": "number"
                },
                "executionBlockOffset": {
                    "type": "number"
                },
                "feeIncrease": {
                    "type": "number"
                },
                "retries": {
                    "type": "number"
                }
            },
            "type": "object"
        },
        "WalletApi": {
            "additionalProperties": false,
            "properties": {
                "url": {
                    "type": "string"
                }
            },
            "required": [
                "url"
            ],
            "type": "object"
        }
    },
    "properties": {
        "$schema": {
            "type": "string"
        },
        "agentBotSettings": {
            "$ref": "#/definitions/AgentBotSettingsJson"
        },
        "apiNotifierConfigs": {
            "items": {
                "$ref": "#/definitions/ApiNotifierConfig"
            },
            "type": "array"
        },
        "assetManagerController": {
            "type": "string"
        },
        "attestationProviderUrls": {
            "items": {
                "type": "string"
            },
            "type": "array"
        },
        "challengeStrategy": {
            "$ref": "#/definitions/BotStrategyDefinition"
        },
        "contractsJsonFile": {
            "type": "string"
        },
        "fAssets": {
            "additionalProperties": {
                "$ref": "#/definitions/BotFAssetInfo"
            },
            "type": "object"
        },
        "liquidationStrategy": {
            "$ref": "#/definitions/BotStrategyDefinition"
        },
        "loopDelay": {
            "type": "number"
        },
        "nativeChainInfo": {
            "$ref": "#/definitions/BotNativeChainInfo"
        },
        "ormOptions": {
            "$ref": "#/definitions/OrmConfigOptions"
        },
        "priceFeedApiUrls": {
            "items": {
                "type": "string"
            },
            "type": "array"
        },
        "pricePublisherMaxDelayMs": {
            "type": "number"
        },
        "prioritizeAddressUpdater": {
            "type": "boolean"
        },
        "rpcUrl": {
            "type": "string"
        },
        "walletOptions": {
            "additionalProperties": false,
            "properties": {
                "blockOffset": {
                    "type": "number"
                },
                "feeIncrease": {
                    "type": "number"
                },
                "retries": {
                    "type": "number"
                }
            },
            "type": "object"
        }
    },
    "required": [
        "agentBotSettings",
        "fAssets",
        "loopDelay",
        "nativeChainInfo",
        "prioritizeAddressUpdater",
        "rpcUrl"
    ],
    "type": "object"
}

