{
    "name": "simple-wallet",
    "version": "1.0.0",
    "description": "Fasset bots simple wallet",
    "engines": {
        "node": ">=14.6.0"
    },
    "exports": {
        ".": {
            "types": "./dist/src/index.d.ts",
            "default": "./dist/src/index.js"
        }
    },
    "author": "Flare Labs",
    "homepage": "https://gitlab.com/flarenetwork/fasset-bots",
    "repository": {
        "type": "git",
        "url": "https://gitlab.com/flarenetwork/fasset-bots"
    },
    "license": "MIT",
    "files": [
        "/dist",
        "/src"
    ],
    "scripts": {
        "---------PUBLISH---SCRIPTS": "",
        "publish": "echo npm publish",
        "prepack": "yarn build",
        "---------BUILD---SCRIPTS": "",
        "prebuild": "rm -rf dist",
        "build": "tsc --allowJs",
        "---------LINT---SCRIPTS": "",
        "lint": "eslint \"src/**/*ts\" \"test/**/*ts\"",
        "---------TEST---SCRIPTS": "",
        "test": "mocha -r ts-node/register -r dotenv/config -r source-map-support/register -t 0",
        "test_all": "mocha -r ts-node/register -r dotenv/config -r source-map-support/register --recursive -t 500000 test/**/*.ts ",
        "test:coverage": "yarn nyc yarn test_all"
    },
    "dependencies": {
        "@types/bitcore-lib": "^0.15.3",
        "@types/bn.js": "5.1.5",
        "@types/bs58check": "^2.1.0",
        "@types/elliptic": "^6.4.14",
        "@types/node": "20.7.0",
        "@types/tiny-secp256k1": "^2.0.1",
        "algosdk": "^2.3.0",
        "axios": "1.5.1",
        "axios-retry": "^3.8.0",
        "bip32": "^3.0.1",
        "bip39": "^3.0.4",
        "bitcoinjs-lib": "^6.0.2",
        "bitcore-lib": "^8.25.36",
        "bitcore-lib-doge": "^8.25.36",
        "bitcore-lib-ltc": "^8.25.36",
        "bn.js": "5.2.1",
        "bs58check": "^3.0.1",
        "dotenv": "16.3.1",
        "elliptic": "^6.5.4",
        "fast-safe-stringify": "^2.1.1",
        "ripple-address-codec": "4.2.4",
        "ripple-binary-codec": "1.4.2",
        "ripple-keypairs": "^1.1.5",
        "source-map-support": "0.5.21",
        "tiny-secp256k1": "^2.2.1",
        "web3-utils": "1.10.2",
        "xrpl": "2.3.1"
    },
    "devDependencies": {
        "typescript": "5.2.2",
        "wallet-address-validator": "^0.2.4"
    },
    "packageManager": "yarn@3.6.4"
}
